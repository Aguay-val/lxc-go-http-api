{
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "schemes": [
    "http",
    "https"
  ],
  "swagger": "2.0",
  "info": {
    "description": "this is to show how to write RESTful APIs in golang.\nthat is to provide a detailed overview of the language specs",
    "title": "LXC HTTP API",
    "contact": {
      "name": "Tim Clerc",
      "email": "tim@clerc.im"
    },
    "version": "0.1"
  },
  "host": "localhost:8080",
  "paths": {
    "/version": {
      "get": {
        "description": "Return current LXC version",
        "produces": [
          "application/json"
        ],
        "tags": [
          "general"
        ],
        "operationId": "getVersion",
        "responses": {
          "200": {
            "description": "Version response",
            "schema": {
              "$ref": "#/responses/Version"
            }
          },
          "default": {
            "description": "unexpected error",
            "schema": {
              "$ref": "#/responses/HTTPClientResp"
            }
          }
        }
      }
    }
  },
  "responses": {
    "HTTPClientResp": {
      "description": "HTTPClientResp format API client response to JSON",
      "headers": {
        "message": {
          "type": "string"
        },
        "status": {
          "type": "string"
        }
      }
    },
    "Version": {
      "description": "Version response payload",
      "schema": {
        "type": "object",
        "required": [
          "version"
        ],
        "properties": {
          "version": {
            "description": "The version number",
            "type": "string",
            "x-go-name": "Version",
            "example": "Expected type int"
          }
        }
      }
    }
  },
  "securityDefinitions": {
    "api_key": {
      "type": "apiKey",
      "name": "KEY",
      "in": "header"
    }
  },
  "security": [
    {
      "api_key": []
    }
  ]
}